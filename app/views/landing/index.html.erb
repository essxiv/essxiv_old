<!-- SUBMIT MESSAGE -->
<section id="sub-msg">
	<div class='container'>

		<div class='row'>
			<div class='col-sm-6 col-sm-offset-3'>
				<div class='service-header'>
					<h2 class='inq-title'>Inquiries</h2>
					<div class='inq-subtitle'>
						Feel free to ask me any questions!
					</div>
				</div>
			</div>
		</div>		

		<div class='row'>
			<div class='col-sm-6 col-sm-offset-3'>
				<form id='contact-form'>

					<div class='ajax-hidden'>

						<div class='form-group wow fadeInUp animated'>
							<label class='sr-only'>Name</label>
							<input type='text' class='form-control' placeholder='Name'>
						</div>

						<div class='form-group wow fadeInUp animated'>
							<label class='sr-only'>Email</label>
							<input type='text' class='form-control' placeholder='E-Mail'>
						</div>

						<div class='form-group wow fadeInUp animated'>
							<textarea class='form-control' placeholder='Message' rows='7'></textarea>
						</div>

						<button type='submit' class='btn btn-lg btn-block wow fadeInUp animated'>Send Message</button>


					</div>


				</form>
			</div>
		</div>

	</div>
</section>


